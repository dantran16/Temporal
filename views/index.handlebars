<html>
<head>
	<title>Final Project</title>
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/main.css" rel="stylesheet">
	<link href="/css/main-theme.css" rel="stylesheet">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	<style> @import url('https://fonts.googleapis.com/css?family=Rubik');</style>
</head>
<body>
	<div class="screen-container">
		<div class="container-header"> 
			<img src="svg/menu.svg" alt="BACK" id='btn-menu'>
			<h1> Dashboard </h1>
			<img src="svg/about.svg" alt='INFO' id='btn-info'>
			<div class="row" id="row">  
				<button class="btn btn-default" id="Calendar"> Calendar </button>
				<button class="btn btn-default" id="Goals"> Goals </button>
				<button class="btn btn-default" id = "Favorites"> Favorites </button>
			</div>
		</div>
		
		<div class="onGoingSessions">
			<h1>ON GOING SESSION</h1>
			<div class="onGoingSessions-box">
				<div class="onGoingSessions-tabs">
						<ul class="list-group" id="Sessions">
							<!-- accesses sessions and access all the variables -->
							{{#each sessions}}
								<li class="friends" id="{{id}}"> {{name}} </li>
							{{/each}}
						</ul>
				</div>
			</div>
		</div>

		<div class="container-footer">
			<button onclick="newSession()" class="btn new-session" id="button-deets"> + New Session </button>
		</div>
	</div>

<script src="https://code.jquery.com/jquery.js"></script>
<!-- Include all compiled plugins (blelow), or include individual files as needed -->
<script src="js/bootstrap.min.js"></script>
<script src="js/newSession.js"></script>
<script src="js/timer.js"></script>

</body>
</html>
